<p id="notice"><%= notice %></p>

<h1>Category Masters</h1>

<table>
  <thead>
    <tr>
      <th>Cat desc</th>
      <th>Comment</th>
      <th>Active status</th>
      <th>Del status</th>
      <th>User</th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category_masters.each do |category_master| %>
      <tr>
        <td><%= category_master.cat_desc %></td>
        <td><%= category_master.comment %></td>
        <td><%= category_master.active_status %></td>
        <td><%= category_master.del_status %></td>
        <td><%= category_master.user_id %></td>
        <td><%= category_master.integer %></td>
        <td><%= link_to 'Show', category_master %></td>
        <td><%= link_to 'Edit', edit_category_master_path(category_master) %></td>
        <td><%= link_to 'Destroy', category_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category Master', new_category_master_path %>
