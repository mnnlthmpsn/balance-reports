<%= form_with(model: generated_report) do |form| %>
  <% if generated_report.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(generated_report.errors.count, "error") %> prohibited this generated_report from being saved:</h2>

      <ul>
        <% generated_report.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :cost_upload_id %>
    <%= form.text_field :cost_upload_id %>
  </div>

  <div class="field">
    <%= form.label :report_file_name %>
    <%= form.text_field :report_file_name %>
  </div>

  <div class="field">
    <%= form.label :report_file_path %>
    <%= form.text_area :report_file_path %>
  </div>

  <div class="field">
    <%= form.label :active_status %>
    <%= form.check_box :active_status %>
  </div>

  <div class="field">
    <%= form.label :del_status %>
    <%= form.check_box :del_status %>
  </div>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.number_field :user_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
